    "build:ca": "webpack --env.region=ca --env.production",
    "build:us": "webpack --env.region=us --env.production",
    "test": "yarn run lint",
    "lint": "run-p lint:css lint:js",
    "lint:css": "stylelint 'client/**/*.less' --fix",
    "lint:js": "eslint --quiet --fix ./client",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.less": [
      "stylelint 'client/**/*.less' --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
