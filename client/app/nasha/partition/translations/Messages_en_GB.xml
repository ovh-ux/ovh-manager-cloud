   <translation id="nasha_partitions_info_NFS" qtlid="345039">NFS partition mounting:</translation>
   <translation id="nasha_partitions_info_linux_NFS" qtlid="345052">mount -t nfs {{ ip }}:/{{ name }}/PARTITION_NAME_1 /mnt/</translation>
   <translation id="nasha_partitions_info_CIFS" qtlid="345065">CIFS partition mounting :</translation>
   <translation id="nasha_partitions_info_linux_CIFS" qtlid="345078">mount -t cifs //{{ ip }}/{{ name }}_PARTITION_NAME_1 /mnt/ -ouid=nobody,gid=nogroup,password=''</translation>
