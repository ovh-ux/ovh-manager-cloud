   <translation id="nasha_partitions_info_NFS" qtlid="345039">Montowanie partycji NFS:</translation>
   <translation id="nasha_partitions_info_linux_NFS" qtlid="345052">mount -t nfs {{ ip }}:/{{ name }}/NAZWA_PARTYCJI_1 /mnt/</translation>
   <translation id="nasha_partitions_info_CIFS" qtlid="345065">Montowanie partycji CIFS:</translation>
   <translation id="nasha_partitions_info_linux_CIFS" qtlid="345078">mount -t cifs //{{ ip }}/{{ name }}_NAZWA_PARTYCJI_1 /mnt/ -ouid=nobody,gid=nogroup,password=''</translation>
