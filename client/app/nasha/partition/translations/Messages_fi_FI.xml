   <translation id="nasha_partitions_info_NFS" qtlid="345039">NFS-levyosion alustus:</translation>
   <translation id="nasha_partitions_info_linux_NFS" qtlid="345052">mount -t nfs {{ ip }}:/{{ name }}/NIMI_LEVYOSIO_1 /mnt/</translation>
