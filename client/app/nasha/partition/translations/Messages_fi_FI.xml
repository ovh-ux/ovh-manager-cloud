   <translation id="nasha_partitions_info_linux_CIFS" qtlid="345078">mount -t cifs //{{ ip }}/{{ name }}_NIMI_LEVYOSIO_1 /mnt/ -ouid=nobody,gid=nogroup,password=''</translation>
