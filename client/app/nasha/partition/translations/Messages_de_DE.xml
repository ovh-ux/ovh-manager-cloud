   <translation id="nasha_partitions_info_NFS" qtlid="345039">Mounten der NFS-Partition: </translation>
   <translation id="nasha_partitions_info_linux_NFS" qtlid="345052">mount -t nfs {{ ip }}:/{{ name }}/NAME_PARTITION_1 /mnt/</translation>
   <translation id="nasha_partitions_info_CIFS" qtlid="345065">Mounten der CIFS-Partition: </translation>
   <translation id="nasha_partitions_info_linux_CIFS" qtlid="345078">mount -t cifs //{{ ip }}/{{ name }}_NAME_PARTITION_1 /mnt/ -ouid=nobody,gid=nogroup,password=''</translation>
