   <translation id="nasha_partitions_info_linux_1" qtlid="345026">Assegure-se que o sistema de ficheiros da sua partição foi corretamente tomada em conta.</translation>
   <translation id="nasha_partitions_info_NFS" qtlid="345039">Montagem da partição NFS:</translation>
   <translation id="nasha_partitions_info_linux_NFS" qtlid="345052">mount -t nfs {{ ip }}:/{{ name }}/NOME_PARTICAO_1 /mnt/</translation>
   <translation id="nasha_partitions_info_CIFS" qtlid="345065">Montagem da partição CIFS:</translation>
   <translation id="nasha_partitions_info_linux_CIFS" qtlid="345078">mount -t cifs //{{ ip }}/{{ name }}_NOME_PARTICAO_1 /mnt/ -ouid=nobody,gid=nogroup,password=''</translation>
