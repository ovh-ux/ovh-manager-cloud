   <translation id="nasha_partitions_info_linux_1" qtlid="345026">Assicurati che il file system della tua partizione venga preso in carico correttamente.</translation>
   <translation id="nasha_partitions_info_NFS" qtlid="345039">Esegui il mount della partizione NFS:</translation>
   <translation id="nasha_partitions_info_linux_NFS" qtlid="345052">mount -t nfs {{ ip }}:/{{ name }}/NOM_PARTITION_1 /mnt/</translation>
   <translation id="nasha_partitions_info_CIFS" qtlid="345065">Esegui il mount della partizione CIFS:</translation>
   <translation id="nasha_partitions_info_linux_CIFS" qtlid="345078">mount -t cifs //{{ ip }}/{{ name }}_NOM_PARTITION_1 /mnt/ -ouid=nobody,gid=nogroup,password=''</translation>
   <translation id="nasha_partitions_info_windows_CIFS" qtlid="345091">net use z: {{ip}}{{name}}_NOM_PARTITION_1</translation>
